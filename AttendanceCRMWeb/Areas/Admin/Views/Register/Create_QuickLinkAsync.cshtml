
@using Repository.Model
@using ViewModel.UserManagement
@model  QuickLinkVm
@{
    Model.IsActive = true;
}

<form id="QuickLinkFrm" class="col-md-12 form-horizontal">
    <div class="col-md-12">
        @if (Model != null)
        {
            @Html.HiddenFor(m => m.Id)
            @Html.HiddenFor(x => x.IsActive)
        }
        <div class="form-group">
            <div class="col-md-3"></div>
            <div class="card col-md-9">
                <div class="card-body p-2 pt-3">
                    <input @string.Format("{0}", Model.LinkType == QuickLinkType.Systemic ? "Checked" : "") data-val="true" id="rb2LinkType" name="LinkType" type="radio" value="@QuickLinkType.Systemic" onclick="LinkTypeChangeEvent()">
                    <label for="rb2LinkType">انتخاب لینکهای سیستم</label>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <input @string.Format("{0}", Model.LinkType == QuickLinkType.External ? "Checked" : "") id="rb1LinkType" name="LinkType" type="radio" value="@QuickLinkType.External" onclick="LinkTypeChangeEvent()">
                    <label for="rb1LinkType">تعریف URL خارجی</label>
                </div>
            </div>
        </div>

        <div id="SystemicDiv">
            @Html.EditorFor(x => x.PubMenu_Id)
        </div>

        <div id="ExternalDiv">
            @Html.EditorFor(x => x.URLAddress)
        </div>

        @Html.EditorFor(x => x.Title)
        @*@Html.EditorFor(x => x.Comment)*@
        @Html.EditorFor(x => x.Priority)





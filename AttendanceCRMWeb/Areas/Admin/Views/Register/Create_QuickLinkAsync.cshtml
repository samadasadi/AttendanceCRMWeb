<script>



    $(function () {
        $('.select2').select2();
        LinkTypeChangeEvent();
    });

    $("#PubMenu_Id").change(function () {
        $("#Title").val($("#PubMenu_Id option:selected").text());

    });

    function LinkTypeChangeEvent() {
        if (document.getElementById('rb1LinkType').checked) {
            $("#SystemicDiv").hide();
            $("#ExternalDiv").show();
        }
        else if (document.getElementById('rb2LinkType').checked) {
            $("#SystemicDiv").show();
            $("#ExternalDiv").hide();
        }
    }

</script>



